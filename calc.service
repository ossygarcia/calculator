[Unit]
Description=Gunicorn instance to serve calc
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/src/calculator/
Environment="PATH=/home/ubuntu/venv/calculator"
ExecStart=/home/ubuntu/venv/bin/ --workers 3 --bind unix:calc.sock -m 007 wsgi:app

[Install]
WantedBy=multi-user.target
